{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jax-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "rules":  {
    "react/prop-types": 0,
    "no-console": 1
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true 
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}